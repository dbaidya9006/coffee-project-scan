    "api"
  ],
  "author": "Your Name",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.ncsu.edu:CSC-519/project.git"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "chai": "^4.3.10",
    "eslint": "^8.53.0",
    "esprima": "^4.0.1",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3"
  }
